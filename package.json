{
  "name": "jest-cov-reporter",
  "version": "0.1.0",
  "description": "Produce a coverage report and provide a diff with base report",
  "main": "src/index.ts",
  "scripts": {
    "lint": "eslint -c .eslintrc.json src",
    "test": "jest src --coverage --verbose",
    "jest-dev": "jest src --coverage --verbose --watch",
    "build": "tsc",
    "pack": "ncc build"
  },
  "repository": "git@github.com:arunshan/jest-cov-reporter.git",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@vercel/ncc": "^0.27.0",
    "babel-jest": "^26.6.3",
    "eslint": "^8.6.0",
    "jest": "^26.6.3",
    "ts-jest": "^24.3.0",
    "typescript": "^3.9.6"
  }
}
